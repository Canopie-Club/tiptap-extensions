{
  "name": "@canopie-club/tiptap-extensions",
  "version": "0.1.0",
  "description": "Custom TipTap extensions for rich text editing",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "tiptap",
    "editor",
    "vue",
    "extensions"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@tiptap/core": "^2.0.0",
    "@tiptap/vue-3": "^2.0.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@tiptap/core": "^2.0.0",
    "@tiptap/vue-3": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vue": "^3.0.0"
  }
}
